<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="triangle" class="com.bridgelabz.SpringAutowiring.Triangle" autowire="byName">
	
	</bean>
	
	<bean id="pointA" class="com.bridgelabz.SpringAutowiring.Point">
	<property name="x" value="0"/>
	<property name="y" value="0"/>
	</bean>
	
	<bean id="pointB" class="com.bridgelabz.SpringAutowiring.Point">
	<property name="x" value="-20"/>
	<property name="y" value="0"/>
	</bean>
	
	<bean id="pointC" class="com.bridgelabz.SpringAutowiring.Point">
	<property name="x" value="20"/>
	<property name="y" value="0"/>
	</bean>
	
	</beans>